{
  "version": "2.1.0",
  "$schema": "http://json.schemastore.org/sarif-2.1.0",
  "runs": [
    {
      "tool": {
        "driver": {
          "name": "HawkScan",
          "version": "0.9.2",
          "semanticVersion": "0.9.2",
          "informationUri": "https://docs.stackhawk.com/hawkscan/",
          "rules": [
            {
              "id": "1",
              "name": "ScanResults",
              "helpUri": "https://docs.stackhawk.com/web-app/scans.html#scan-details-page",
              "help": {
                "text": "Provides a URL to the scan results on the StackHawk platform"
              }
            }
          ]
        }
      },
      "results": [
        {
          "locations": [
            {
              "id": 1,
              "physicalLocation": {
                "region": {
                  "startLine": 1
                },
                "artifactLocation": {
                  "uri": "README.md"
                }
              }
            }
          ],
          "message": {
            "text": "Hey, great job."
          },
          "ruleId": "1",
          "properties": {
            "Confidence": 5.0
          }
        }
      ],
      "artifacts": [
        {
          "location": {
            "uri": "https://app.stackhawk.com/scans/a0493c56-67f3-44d4-86a9-371fb22a63af"
          }
        }
      ]
    }
  ]
}
