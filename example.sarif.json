{
  "version": "2.1.0",
  "$schema": "http://json.schemastore.org/sarif-2.1.0",
  "runs": [
    {
      "tool": {
        "driver": {
          "name": "HawkScan",
          "version": "0.9.2",
          "semanticVersion": "0.9.2",
          "informationUri": "https://docs.stackhawk.com/hawkscan/",
          "rules": [
            {
              "id": "alert/threshold-met",
              "name": "alert/threshold-met",
              "helpUri": "https://docs.stackhawk.com/web-app/scans.html#scan-details-page",
              "help": {
                "text": "HawkScan found issues that meet or exceed your alert threshold, `hawk.failureThreshold`"
              },
              "shortDescription": {
                "text": "HawkScan found issues that meet or exceed your alert threshold"
              },
              "properties": {
                "tags": [
                  "Alert Threshold Met"
                ]
              }
            }
          ]
        }
      },
      "results": [
        {
          "level": "warning",
          "locations": [
            {
              "id": 1,
              "physicalLocation": {
                "region": {
                  "startLine": 1
                },
                "artifactLocation": {
                  "uri": "nofile.md"
                }
              }
            }
          ],
          "message": {
            "text": "HawkScan found issues that meet or exceed your failure threshold.\n`hawk.failureThreshold=MEDIUM`.\nSee [Scan Results](https://app.stackhawk.com/scans/79cb89ac-0686-406a-aecd-74c2bea68359) for more details."
          },
          "ruleId": "alert/threshold-met"
        }
      ]
    }
  ]
}
